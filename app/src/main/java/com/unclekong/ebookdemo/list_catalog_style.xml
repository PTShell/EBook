<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/linearLayout1"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:background="@drawable/category_selector"
	xmlns:android="http://schemas.android.com/apk/res/android">
	<AbsoluteLayout android:id="@+id/widget35"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		xmlns:android="http://schemas.android.com/apk/res/android">
		<ImageView android:id="@+id/image" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:layout_x="10dp"
			 android:layout_y="5dp" android:contentDescription="@string/app_img">
		</ImageView>
	</AbsoluteLayout>
	<LinearLayout android:id="@+id/linewr2"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:orientation="vertical">
		<TextView android:id="@+id/textcalalog" android:layout_width="wrap_content"
			android:textColor="@color/white" android:layout_height="wrap_content"
			android:textSize="16dp" android:layout_marginLeft="30dp">
		</TextView>
	</LinearLayout>
</LinearLayout>
